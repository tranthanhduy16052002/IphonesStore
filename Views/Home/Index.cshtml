@model IphonesStore.Models.ViewModels.IphonesListViewModel
@foreach (var p in Model.Iphones)
{
<partial name="IphoneTemplate" model="p" />
}

<div  page-model="@Model.PagingInfo"  page-action="Index"  page-classes-enabled="true"
page-class="btn" page-class-normal="btn-outline-dark"
page-class-selected="btn-primary" page-url-genre="@Model.CurrentGenre" class="btn-group  pull-right m-1">
</div>
